<h3> Usuários </h3>

  <table border="1">
  <caption> Usuários Cadastrados </caption>
  <thead>
    <tr>
      <th width="150"> Identificador </th>
      <th width="150"> Nome </th>
      <th width="150"> E-mail </th>
      <th width="150"> CPF </th>
    </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td style="text-align: center;"><%= link_to user.id, user %></td>
      <td style="text-align: right;"><%= user.name %></td>
      <td style="text-align: right;"><%= user.email %></td>
      <td style="text-align: right;"><%= user.cpf %></td>
    </tr>
  <% end %>
  </tbody>
  </table>